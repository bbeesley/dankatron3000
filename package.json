{
  "name": "dankatron3000",
  "version": "1.0.0",
  "description": "Somthing dank this way comes",
  "main": "dist/main/index.js",
  "repository": "git@github.com:bbeesley/dankatron3000.git",
  "author": "Bill Beesley <bbeesley@me.com>",
  "license": "MIT",
  "scripts": {
    "start-dev": "NODE_ENV=development nodemon --watch 'src/main/**'",
    "start": "node ./dist/main/index.js",
    "clean": "rm -rf dist/*",
    "compile": "BABEL_ENV=production babel src --out-dir dist --copy-files --source-maps --extensions \".js\" --ignore \"src/test/**\"",
    "lint": "./node_modules/.bin/eslint src/**  --fix",
    "mocha": "BABEL_ENV=test nyc mocha --require ./src/test/index.js 'src/test/unit/*.test.js'",
    "flow": "flow",
    "flow:delete": "find ./dist/main -name *.flow -delete || :",
    "flow:generate": "flow-copy-source -v ./src/main ./dist/main",
    "pretest": "yarn flow:delete && run-p clean lint flow",
    "posttest": "yarn compile && yarn flow:generate",
    "test": "BABEL_ENV=test yarn mocha"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bunyan": "^1.8.12",
    "koa": "^2.5.1",
    "koa-bunyan-logger": "^2.0.0",
    "koa-router": "^7.4.0",
    "node-fetch": "^2.1.2",
    "regenerator-runtime": "^0.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-template": "^6.26.0",
    "babel-types": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.71.0",
    "flow-copy-source": "^1.3.0",
    "nodemon": "^1.17.4",
    "npm-run-all": "^4.1.3"
  }
}
